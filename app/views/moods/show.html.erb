<div class="page-header">
  <h3>Mood #<%= @mood.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Mood choice</dt>
      <dd><%= @mood.mood_choice %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/moods" class="btn btn-primary">
        Back
      </a>
      <a href="/moods/<%= @mood.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_mood/<%= @mood.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
